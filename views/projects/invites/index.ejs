<!DOCTYPE html>
<html lang="en">
  <%- include('../../partials/head') %>
  <body>
    <%- include('../../partials/header') %>
    <div class="container page">
      <div class="row">
        <div class="col-12 col-md-8 order-2 order-md-1">
          <% usersList.forEach((userInList)=>{%>
            <div class="card page-card">
              <div class="card-body">
                <div class="card-title card-heading">
                   <h3 class=" inline-username"><%= userInList.username %></h3>
                   <img
                     src="../../<%= userInList.thumbnail %>"
                     alt="<%= userInList.username %>"
                     class="thumbnail-img-sm"
                   />
                </div>
                <a href="/projects/<%= project._id%>/invites/<%= userInList._id %>">
                  <button class="form-button">inviteUser</button></a>
              </div>
            </div>
            <% })%>
          </div>
          <%- include('../../partials/sidebar') %>
        </div>
      </div>
    </div>
  </body>
</html>
